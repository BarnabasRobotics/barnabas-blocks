{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "allowJs": true,
        "module": "ESNext",
        "target": "ESNext",
        "declaration": false,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "importHelpers": true,
        "resolveJsonModule": true
    },
}